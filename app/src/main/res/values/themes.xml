        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/azul_o</item>
        <item name="colorPrimaryVariant">@color/azul_c</item>
        <item name="colorOnPrimary">@color/blanco</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dorado_o</item>
        <item name="colorSecondaryVariant">@color/dorado_c</item>
        <item name="colorOnSecondary">@color/negro</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>