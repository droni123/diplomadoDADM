<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".entregables.Entregable2MainActivity">
    <TextView
        android:text="@string/e3nombrepet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e2tituloPet"
        android:textAlignment="center"
        android:textSize="40sp"
        android:padding="15dp"
        android:background="@color/azul_o"
        android:textColor="@color/blanco"
        android:fontFamily="@font/mythias"
        />
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e2ContenedorFragment"
        android:layout_below="@id/e2tituloPet"
        />
</RelativeLayout>