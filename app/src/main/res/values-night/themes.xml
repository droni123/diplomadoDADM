<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. .DarkActionBar -->
    <style name="Theme.AppDiplomado" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/azul_cc</item>
        <item name="colorPrimaryVariant">@color/azul_c</item>
        <item name="colorOnPrimary">@color/azul_o</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dorado_o</item>
        <item name="colorSecondaryVariant">@color/dorado_o</item>
        <item name="colorOnSecondary">@color/negro</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TemaEjemplo" parent="Theme.AppDiplomado">
        <item name="android:statusBarColor">@color/orage</item>
    </style>
    <style name="Theme.TemaEjemplo2" parent="Theme.AppDiplomado">
        <item name="colorSecondary">@color/orage</item>
        <item name="colorOnSecondary">@color/blue</item>
        <item name="fontFamily">@font/bernier_shade_regular</item>
    </style>
</resources>